{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "array",
  "items": {
    "type": "object",
    "properties": {
      "id": {
        "type": "integer"
      },
      "isActive": {
        "type": "boolean"
      },
      "createdAt": {
        "type": "string",
        "format": "date-time"
      },
      "updatedAt": {
        "type": "string",
        "format": "date-time"
      },
      "deletedAt": {
        "type": ["string", "null"],
        "format": "date-time"
      },
      "name": {
        "type": "string"
      }
    },
    "required": ["id", "isActive", "createdAt", "updatedAt", "name"]
  }
}